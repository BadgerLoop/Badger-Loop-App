<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.ionsocial627948" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>BadgerLoop</name>
  <description>
        BadgerLoop App
    </description>
  <author email="croossin94@gmail.com" href="https://badgerloop.com/">
      BadgerLoop
    </author>
  <content src="index.html"/>
  <access origin="*"/>
